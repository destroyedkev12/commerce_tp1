<h1>Clients Has Employeurs</h1>

<table>
  <thead>
    <tr>
      <th>Clients</th>
      <th>Employeurs</th>
      <th>Datedebut</th>
      <th>Datefin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients_has_employeurs.each do |clients_has_employeur| %>
      <tr>
        <td><%= clients_has_employeur.Clients_id %></td>
        <td><%= clients_has_employeur.Employeurs_id %></td>
        <td><%= clients_has_employeur.DateDebut %></td>
        <td><%= clients_has_employeur.DateFin %></td>
        <td><%= link_to 'Show', clients_has_employeur %></td>
        <td><%= link_to 'Edit', edit_clients_has_employeur_path(clients_has_employeur) %></td>
        <td><%= link_to 'Destroy', clients_has_employeur, method: :delete, data: { confirm: 'Are you sure?' } %></td>
