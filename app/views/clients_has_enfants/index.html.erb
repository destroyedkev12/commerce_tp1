<h1>Clients Has Enfants</h1>

<table>
  <thead>
    <tr>
      <th>Enfants</th>
      <th>Clients</th>
      <th>Lien</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients_has_enfants.each do |clients_has_enfant| %>
      <tr>
        <td><%= clients_has_enfant.Enfants_id %></td>
        <td><%= clients_has_enfant.Clients_id %></td>
        <td><%= clients_has_enfant.Lien %></td>
        <td><%= link_to 'Show', clients_has_enfant %></td>
        <td><%= link_to 'Edit', edit_clients_has_enfant_path(clients_has_enfant) %></td>
        <td><%= link_to 'Destroy', clients_has_enfant, method: :delete, data: { confirm: 'Are you sure?' } %></td>
